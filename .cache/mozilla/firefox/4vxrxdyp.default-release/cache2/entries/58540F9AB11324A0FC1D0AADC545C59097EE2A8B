     style="display:block"
     data-ad-format="autorelaxed"
     data-ad-client="ca-pub-2239642260066658"
     data-ad-slot="8415903617"></ins>
<script>
     (adsbygoogle = window.adsbygoogle || []).push({});
</script>                                                     <script>
